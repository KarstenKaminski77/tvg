        {% block javascripts %}
            <!-- jQuery -->
            <script src="{{ asset('js/jquery-3.6.0.min.js') }}"></script>
            <script src="{{ asset('js/bootstrap.bundle.min.js') }}"></script>
            <script src="{{ asset('js/jquery.session.min.js') }}"></script>
            <script src="{{ asset('js/intlTelInput.min.js') }}"></script>

            <script>


                $(window).on('load', function(){

                    history.pushState(null, null, window.location.href);
                    history.back();
                    window.onpopstate = () => history.forward();

                   if($.session.get('search_key') != '' || $.session.get('search_key') != 'udefined'){

                        let page_no = 1;
                        let keyword = $.session.get('search_key');

                        $.ajax({
                            url: "{{ path('search_inventory') }}",
                            type: 'POST',
                            data: {
                                keyword: keyword,
                                page_no: page_no
                            },
                            success: function (response) {

                                if(window.location.pathname == '/clinics/inventory') {

                                    $('#search_field').val(keyword);
                                    $('#inventory').empty().append(response);
                                    $('.container').removeClass('mt-5').addClass('mt-5');
                                    document.title = 'Fluid';
                                    window.history.pushState({
                                        "html": response.html,
                                        "pageTitle": 'Fluid'
                                    }, "", 'https://127.0.0.1:8000/clinics/inventory');
                                    $.session.set('search_key', keyword);
                                }
                            }
                        });
                    }
                });
                $(document).ready(function (){

                    {# Inventory Search #}
                    let page_no = 0;
                    let form_data = '';

                    $(document).on('click','.page-link',function (e){

                        page_no = $(this).data('page-id');
                    });
                    $(document).on('click','#search_btn',function (e){

                        page_no = 1;
                    });
                    $(document).on('click','.view-list',function (e){

                        e.preventDefault();

                        $('#search_field').val('');
                    });
                    $(document).on('click','#search_btn, .page-link, .view-list',function (e){

                        e.preventDefault();

                        let keyword = '';
                        let list_id = '';
                        let page_id = $(this).data('page-id');

                        if($('#search_field').val() != '' || $('#search_field').val() != 'undefined') {

                            keyword = $('#search_field').val();
                        }

                        if($(this).data('list-id') != '' || $(this).data('list-id') != 'undefined'){

                            list_id = $(this).data('list-id');
                        }
                        let cat = $($('input[name="category[]"]')).serializeArray();
                        let filter = $($('input[name="filter[]"]')).serializeArray();
                        let manufacturer = $($('input[name="manufacturer[]"]')).serializeArray();
                        let distributor = $($('input[name="distributor[]"]')).serializeArray();

                        console.log(cat[0]);
                        $.ajax({
                            url: "{{ path('search_inventory') }}",
                            type: 'POST',
                            dataType: 'json',
                            data: {
                                keyword: keyword,
                                list_id: list_id,
                                page_no: page_no,
                                page_id: page_id,
                                cat: cat,
                                filter: filter,
                                manufacturer: manufacturer,
                                distributor: distributor
                            },
                            success: function (response) {
                                $('#inventory').empty().append(response).show();
                                $('.container').removeClass('mt-5').addClass('mt-5');
                                $('#basket_container').empty().hide();
                                $('#inventory_container').show();
                                document.title = 'Fluid';
                                window.history.pushState({"html":response.html,"pageTitle":'TEST'},"", 'https://127.0.0.1:8000/clinics/inventory');
                                $.session.set('search_key', keyword);
                                popOver();
                            }
                        });
                    });
                    $(document).on('click', '.favourite', function (e){

                        e.preventDefault();

                        let product_id = $(this).data('product-id');

                        $.ajax({
                            url: "{{ path('product_favourite') }}",
                            type: 'POST',
                            data: {
                                product_id: product_id
                            },
                            success: function (response) {

                                if(response == true){

                                    $('#favourite_'+ product_id).removeClass('icon-unchecked').addClass('text-secondary');

                                } else {

                                    $('#favourite_'+ product_id).removeClass('text-secondary').addClass('icon-unchecked');
                                }
                            }
                        });
                    });
                    $(document).on('click','.back-to-search', function (e){

                        e.preventDefault();

                        $('.review_panel').hide();
                        $('.search-panels-container').hide();
                        $('#basket_container').empty().hide();
                        $('#back_btn').addClass('d-none').removeClass('d-flex');
                        $('#inventory').show();
                        $('#inventory_container').show();
                    });
                    $(document).on('change',$('input:checkbox:checked'),function (){

                        let checked = $('input:checkbox:checked').length;

                        if(checked == 0){

                            $('#filter_count').empty().removeClass('ms-1');

                        } else {

                            $('#filter_count').empty().append('('+ checked +')').addClass('ms-1');
                        }
                    });
                    function getBackButton(){

                        let button = '<div class="col-12">';
                        button += '<button class="btn btn-sm btn-primary back-to-search mb-3 w-sm-100 w-auto">';
                        button += '<i class="fa-solid fa-circle-chevron-left me-3"></i>Back To Search</button>';
                        button += '</div>';

                        $('#back_btn').empty().append(button).removeClass('d-none').addClass('d-flex');
                    }
                    {# End Inventory Search #}

                    {# Basket #}
                    {# Add To Basket #}
                    $(document).on('submit', 'form[name="form_add_to_basket"]', function (e){

                        e.preventDefault();

                        let data = new FormData(this);
                        let product_id = data.get('product_id');
                        let distributor_id = data.get('distributor_id');
                        let qty = data.get('qty');
                        let qty_error = $('#error_qty_'+ product_id +'_'+ distributor_id);
                        let is_valid = true;

                        qty_error.hide()

                        if(qty == '' || qty == 'undefined'){

                            qty_error.show();
                            is_valid = false;
                        }

                        if(is_valid) {

                            $.ajax({
                                url: "{{ path('inventory_add_to_basket') }}",
                                type: 'POST',
                                contentType: false,
                                processData: false,
                                cache: false,
                                timeout: 600000,
                                dataType: 'json',
                                data: data,
                                success: function (response) {

                                    if(response.error.length == 0) {

                                        getFlash(response.message)
                                        $('#basket_count').empty().append('('+ response.basket_count +')');
                                        $('#modal_add_to_basket_' + response.product_id + '_' + response.distributor_id).modal('toggle').modal('hide');
                                        $('.modal-backdrop').removeClass('modal-backdrop');
                                        $('.fade').removeClass('fade');

                                    } else {

                                        $('#error_stock_'+ response.product_id +'_'+ response.distributor_id).show().empty().append(response.error);
                                    }
                                }
                            });
                        }
                    });
                    {# End Add To Basket #}

                    {# Update Basket #}
                    function getBasket(basket_id){
                        $.ajax({
                            url: "{{ path('get_basket') }}",
                            type: 'POST',
                            dataType: 'json',
                            data: {
                                basket_id: basket_id
                            },
                            success: function (response) {
                                $('#back_btn').hide();
                                $('#inventory_container').hide();
                                $('#basket_container').empty().append(response).fadeIn(500);
                                $("#basket_container").css({ overflow:"auto" }).addClass('col-container');
                                $('#saved_items_container').hide();
                                $("div.spanner").removeClass("show");
                                $("div.overlay").removeClass("show");

                                popOver();
                            }
                        });
                    }
                    function getFlash(flash){
                        $('#flash').addClass('alert-success').removeClass('alert-danger').addClass('alert').addClass('text-center');
                        $('#flash').removeClass('users-flash').addClass('users-flash').empty().append(flash).removeClass('hidden');

                        setTimeout(function() {
                            $('#flash').removeClass('alert-success').removeClass('alert').removeClass('text-center');
                            $('#flash').removeClass('users-flash').empty().addClass('hidden');
                        }, 5000);
                    }
                    function getSavedBaskets(e){

                        e.preventDefault();

                        $.ajax({
                            url: "{{ path('get_saved_baskets') }}",
                            type: 'POST',
                            dataType: 'json',
                            data: {
                                basket_id: 10
                            },
                            beforeSend: function () {
                                $("div.spanner").addClass("show");
                                $("div.overlay").addClass("show");
                            },
                            success: function (response) {

                                let basket_id = 10;
                                //let flash = response.message;

                                $("div.spanner").removeClass("show");
                                $("div.overlay").removeClass("show");
                                $('#basket_items').empty().append(response);
                                //getFlash(flash);
                            }
                        });
                    }
                    let basket_qty = '';
                    $(document).on('click', '.basket-qty, .modal-basket-qty', function (){

                        basket_qty = $(this).val();
                        $(this).val('');
                    });
                    $(document).on('change', '.basket-qty', function (){

                        let item_id = $(this).data('basket-item-id');
                        let qty = $(this).val();
                        let is_valid = true;

                        if(is_valid) {

                            $.ajax({
                                url: "{{ path('inventory_update_basket') }}",
                                type: 'POST',
                                dataType: 'json',
                                data: {
                                    item_id: item_id,
                                    qty: qty
                                },
                                beforeSend: function (){
                                    $("div.spanner").addClass("show");
                                    $("div.overlay").addClass("show");
                                },
                                success: function (response) {

                                    if(response.error.length == 0) {

                                        $('#inventory_container').fadeOut(900);

                                        let basket_id = response.basket_id;

                                        getBasket(basket_id);

                                    } else {

                                        $("div.spanner").removeClass("show");
                                        $("div.overlay").removeClass("show");
                                        $('#stock_count_error_'+ response.item_id).show().empty().append(response.error);
                                    }
                                }
                            });
                        }
                    });
                    $(document).on('change', '.modal-basket-qty', function (){

                        basket_qty = $(this).val();
                    });
                    $(document).on('mouseout', '.modal-basket-qty', function (){

                        $(this).val(basket_qty);

                    });
                    $(document).on('click', '.remove-item', function (e){

                        e.preventDefault();

                        let item_id = $(this).data('item-id');

                        if(item_id > 0) {

                            $.ajax({
                                url: "{{ path('inventory_remove_basket_item') }}",
                                type: 'POST',
                                dataType: 'json',
                                data: {
                                    item_id: item_id
                                },
                                beforeSend: function (){
                                    $("div.spanner").addClass("show");
                                    $("div.overlay").addClass("show");
                                },
                                success: function (response) {
                                    $('#inventory_container').hide();

                                    let basket_id = response.basket_id;

                                    getBasket(basket_id);
                                }
                            });
                        }
                    });
                    $(document).on('click', '.clear-basket', function (e){

                        e.preventDefault();

                        let basket_id = $(this).data('basket-id');

                        if(basket_id > 0) {

                            $.ajax({
                                url: "{{ path('inventory_clear_basket') }}",
                                type: 'POST',
                                dataType: 'json',
                                data: {
                                    basket_id: basket_id
                                },
                                beforeSend: function (){
                                    $("div.spanner").addClass("show");
                                    $("div.overlay").addClass("show");
                                },
                                success: function (response) {
                                    $('#inventory_container').hide();

                                    let basket_id = response.basket_id;

                                    getBasket(basket_id);
                                }
                            });
                        }
                    });
                    $(document).on('click', '.refresh-basket', function (e){

                        e.preventDefault();

                        let basket_id = $(this).data('basket-id');

                        if(basket_id > 0) {

                            let flash = '<b><i class="fas fa-check-circle"></i> Basket updated.<div class="flash-close"><i class="fa-solid fa-xmark"></i></div>';

                            getBasket(basket_id);
                            getFlash(flash);
                        }
                    });
                    $(document).on('click', '#print_basket', function(e) {

                        $('.basket-qty').each(function (){
                            $(this).replaceWith($(this).val())
                        });

                        $('#btn_checkout').hide();

                        let basket = $('#basket_items').html();
                        let htm = '<!DOCTYPE html><html lang="en"><head><meta charset="utf-8"><title>Fluid Inventory Search</title>';
                        htm += '<link rel="stylesheet" media="all" href="{{ asset('css/bootstrap.min.css') }}"><link rel="stylesheet" media="all" href="{{ asset('css/style.min.css') }}">';
                        htm += '<style type="text/css" media="print">@page { size: landscape; }</style>';
                        htm += '<link rel="stylesheet" href="/css/fontawesome/css/all.min.css"></head><body><div class="container-fluid">'+ basket;
                        htm += '</container></body></html>';

                        let w = window.open( '', "Customer Listing", "menubar=0,location=0,height=670,width=700" );
                        w.document.write(htm);

                        w.print();

                        w.close();
                    });
                    $(document).on('click', '.save-item', function(e) {

                        e.preventDefault();

                        let product_id = $(this).data('product-id');
                        let distributor_id = $(this).data('distributor-id');
                        let item_id = $(this).data('item-id');

                        if(distributor_id > 0 && product_id > 0) {

                            $.ajax({
                                url: "{{ path('save_item') }}",
                                type: 'POST',
                                dataType: 'json',
                                data: {
                                    product_id: product_id,
                                    distributor_id: distributor_id,
                                    item_id: item_id
                                },
                                beforeSend: function (){
                                    $("div.spanner").addClass("show");
                                    $("div.overlay").addClass("show");
                                },
                                success: function (response) {

                                    let basket_id = response.basket_id;
                                    let flash = response.message;

                                    getBasket(basket_id);
                                    getFlash(flash);
                                }
                            });
                        }
                    });
                    $(document).on('click', '.restore-item', function(e) {

                        e.preventDefault();

                        let product_id = $(this).data('product-id');
                        let distributor_id = $(this).data('distributor-id');
                        let item_id = $(this).data('item-id');
                        let basket_id = $(this).data('basket-id');

                        if(distributor_id > 0 && product_id > 0) {

                            $.ajax({
                                url: "{{ path('restore_item') }}",
                                type: 'POST',
                                dataType: 'json',
                                data: {
                                    product_id: product_id,
                                    distributor_id: distributor_id,
                                    item_id: item_id,
                                    basket_id: basket_id
                                },
                                beforeSend: function (){
                                    $("div.spanner").addClass("show");
                                    $("div.overlay").addClass("show");
                                },
                                success: function (response) {

                                    let basket_id = response.basket_id;
                                    let flash = response.message;

                                    getBasket(basket_id);
                                    getFlash(flash);
                                }
                            });
                        }
                    });
                    $(document).on('click', '.remove-saved-item', function(e) {

                        e.preventDefault();

                        let item_id = $(this).data('item-id');

                        if(item_id > 0) {

                            $.ajax({
                                url: "{{ path('remove_saved_item') }}",
                                type: 'POST',
                                dataType: 'json',
                                data: {
                                    item_id: item_id
                                },
                                beforeSend: function (){
                                    $("div.spanner").addClass("show");
                                    $("div.overlay").addClass("show");
                                },
                                success: function (response) {

                                    let basket_id = response.basket_id;
                                    let flash = response.message;

                                    getBasket(basket_id);
                                    getFlash(flash);
                                }
                            });
                        }
                    });
                    $(document).on('click', '.save-all-items', function(e) {

                        e.preventDefault();

                        let basket_id = $(this).data('basket-id');

                        if(basket_id > 0) {

                            $.ajax({
                                url: "{{ path('save_all_items') }}",
                                type: 'POST',
                                dataType: 'json',
                                data: {
                                    basket_id: basket_id
                                },
                                beforeSend: function (){
                                    $("div.spanner").addClass("show");
                                    $("div.overlay").addClass("show");
                                },
                                success: function (response) {

                                    let basket_id = response.basket_id;
                                    let flash = response.message;

                                    getBasket(basket_id);
                                    getFlash(flash);
                                }
                            });
                        }
                    });
                    $(document).on('click', '#saved_items_link', function (e){

                        e.preventDefault();

                        if($('#saved_items_container:visible').length) {

                            $('#saved_items_container').hide(700);

                        } else {

                        $('#saved_items_container').show(700);
                        }
                    });
                    $(document).on('submit', 'form[name="form_save_basket"]', function (e){

                        e.preventDefault();

                        let btn = $(this).find("button[type=submit]:focus" ).html();
                        let clear = 0;

                        if(btn == 'SAVE AND CLEAR'){

                            clear = 1;
                        }

                        $("<input />").attr("type", "hidden").attr("name", "clear").attr("value", clear).appendTo("#form_save_basket");

                        let data = new FormData(this);
                        let basket_name = $('#basket_name').val();
                        let error_basket_name = $('#error_basket_name');
                        let is_valid = true;

                        error_basket_name.hide();

                        if(basket_name == '' || basket_name == 'undefined'){

                            error_basket_name.show();
                            is_valid = false;
                        }

                        if(is_valid) {

                            error_basket_name.hide();

                            $.ajax({
                                url: "{{ path('save_basket') }}",
                                type: 'POST',
                                contentType: false,
                                processData: false,
                                cache: false,
                                timeout: 600000,
                                dataType: 'json',
                                data: data,
                                success: function (response) {
                                    getFlash(response.message);
                                    $('#basket_count').empty().append('('+ response.basket_count +')');
                                    $('#modal_save_basket').modal('toggle').modal('hide');
                                    $('.modal-backdrop').removeClass('modal-backdrop');
                                    $('.fade').removeClass('fade');
                                    getBasket(response.basket_id);
                                }
                            });
                        }
                    });
                    $(document).on('click', '#restore_all', function (e){

                        e.preventDefault();

                        let basket_id = $(this).data('basket-id');

                        if(basket_id > 0) {

                            $.ajax({
                                url: "{{ path('restore_all_items') }}",
                                type: 'POST',
                                dataType: 'json',
                                data: {
                                    basket_id: basket_id
                                },
                                beforeSend: function () {
                                    $("div.spanner").addClass("show");
                                    $("div.overlay").addClass("show");
                                },
                                success: function (response) {

                                    let basket_id = response.basket_id;
                                    let flash = response.message;

                                    getBasket(basket_id);
                                    getFlash(flash);
                                }
                            });
                        }
                    });
                    $(document).on('click', '.saved_baskets_link', function (e){

                        e.preventDefault();

                        let basket_id = $(this).data('basket-id');

                        $.ajax({
                            url: "{{ path('get_saved_baskets') }}",
                            type: 'POST',
                            dataType: 'json',
                            data: {
                                basket_id: basket_id
                            },
                            beforeSend: function () {
                                $("div.spanner").addClass("show");
                                $("div.overlay").addClass("show");
                            },
                            success: function (response) {

                                $("div.spanner").removeClass("show");
                                $("div.overlay").removeClass("show");
                                $('#basket_items').empty().append(response);

                                $('.saved-basket-panel').each(function (){

                                    $('.saved-basket-panel').hide();
                                });
                            }
                        });
                    });
                    $(document).on('click', '.basket-edit', function (e){

                        e.preventDefault();

                        let basket_id = $(this).data('basket-id');

                        $('#basket_name_string_'+ basket_id).hide();
                        $('#basket_name_input_'+ basket_id).show();
                        $(this).replaceWith('<a href="" class="update-saved-basket" id="update_saved_basket_'+ basket_id +'" data-basket-id="'+ basket_id +'"><i class="fa-solid fa-floppy-disk float-end me-3"></i></a>');
                        $('#saved_basket_first_'+ basket_id).removeClass('saved-basket-link');
                    });
                    $(document).on('click', '.update-saved-basket', function (e){

                        e.preventDefault();

                        let basket_id = $(this).data('basket-id');
                        let basket_name = $('#basket_name_'+ basket_id).val();

                        $.ajax({
                            url: "{{ path('update_saved_baskets') }}",
                            type: 'POST',
                            dataType: 'json',
                            data: {
                                basket_id:basket_id,
                                basket_name:basket_name
                            },
                            beforeSend: function () {
                                $("div.spanner").addClass("show");
                                $("div.overlay").addClass("show");
                            },
                            success: function (response) {

                                $("div.spanner").removeClass("show");
                                $("div.overlay").removeClass("show");
                                $('#basket_name_string_'+ basket_id).empty().append(basket_name).show();
                                $('#basket_name_input_'+ basket_id).hide();
                                $('#update_saved_basket_'+ basket_id).replaceWith('<a href="" class="basket-edit" data-basket-id="'+ basket_id +'"><i class="fa-solid fa-pencil float-end me-3"></i></a>');
                                $('#basket_left_col').empty().append(response);
                                $('#saved_basket_first_'+ basket_id).addClass('saved-basket-link');
                            }
                        });
                    });
                    $(document).on('click', '.saved-basket-link', function (e){

                        e.preventDefault();

                        let basket_id = $(this).data('basket-id');
                        $('.saved-basket-link').removeClass('bg-secondary');
                        $('.saved-basket-panel').hide(700);

                        $.ajax({
                            url: "{{ path('get_saved_basket_details') }}",
                            type: 'POST',
                            dataType: 'json',
                            data: {
                                basket_id:basket_id
                            },
                            beforeSend: function () {
                                $("div.spanner").addClass("show");
                                $("div.overlay").addClass("show");
                            },
                            success: function (response) {

                                $('#saved_basket_panel_'+ basket_id).empty().append(response);
                                $("div.spanner").removeClass("show");
                                $("div.overlay").removeClass("show");

                                $('.saved_basket_header').removeClass('bg-secondary');

                                if($('#saved_basket_panel_'+ basket_id +':visible').length) {

                                    $('#saved_basket_panel_' + basket_id).hide(700);


                                } else {

                                    $('#saved_basket_panel_' + basket_id).show(700);
